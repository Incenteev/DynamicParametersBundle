{
    "name": "incenteev/dynamic-parameters-bundle",
    "description": "Runtime retrieval of parameters from environment variables for Symfony",
    "keywords": ["symfony parameters", "environment variable"],
    "homepage": "https://github.com/Incenteev/DynamicParametersBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        }
    ],
    "require": {
        "symfony/dependency-injection": "^2.6.2@dev",
        "symfony/expression-language": "~2.6",
        "symfony/http-kernel": "~2.3",
        "symfony/config": "~2.3"
    },
    "autoload": {
        "psr-4": { "Incenteev\\DynamicParametersBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Incenteev\\DynamicParametersBundle\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
